<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <!-- SLIDER -->
        <parameter key="bootstrapsliderblock.external_javascripts.cms" type="collection">
            <parameter>@TwitterBootstrapBundle/Resources/public/slider/js/slider_editor.js</parameter>
        </parameter>
        
        <parameter key="bootstrapsliderblock.internal_javascripts" type="collection">
            <parameter>$(".carousel").carousel("pause");</parameter>
        </parameter>
        
        <parameter key="bootstrap_slider_block.block.class">RedKiteCms\Block\TwitterBootstrapBundle\Core\Block\Slider\AlBlockManagerBootstrapSliderBlock</parameter>
        <parameter key="bootstrapsliderblock.form.class">RedKiteCms\Block\TwitterBootstrapBundle\Core\Form\Slider\AlSliderType</parameter>
        
        <!-- LABEL --> 
        <parameter key="bootstrap_label_block.block.class">RedKiteCms\Block\TwitterBootstrapBundle\Core\Block\Label\AlBlockManagerBootstrapLabelBlock</parameter>
        <parameter key="bootstrap_label_block.form.class">RedKiteCms\Block\TwitterBootstrapBundle\Core\Form\Label\AlLabelType</parameter>
        
        <!-- BADGE -->         
        <parameter key="bootstrap_badge_block.block.class">RedKiteCms\Block\TwitterBootstrapBundle\Core\Block\Badge\AlBlockManagerBootstrapBadgeBlock</parameter>
        <parameter key="bootstrap_badge_block.form.class">RedKiteCms\Block\TwitterBootstrapBundle\Core\Form\Badge\AlBadgeType</parameter>
        
        <!-- ACORDION -->
        <parameter key="bootstrapaccordionblock.external_javascripts.cms" type="collection">
            <parameter>@TwitterBootstrapBundle/Resources/public/accordion/js/al_accordion.js</parameter>
        </parameter>

        <parameter key="bootstrap_accordion_block.block.class">RedKiteCms\Block\TwitterBootstrapBundle\Core\Block\Accordion\AlBlockManagerBootstrapAccordionBlock</parameter>
        
        <!-- NAVBAR -->
        <parameter key="bootstrapnavbarblock.external_javascripts.cms" type="collection">
            <parameter>@TwitterBootstrapBundle/Resources/public/navbar/js/navbar_editor.js</parameter>
        </parameter>
        
        <parameter key="bootstrap_navbar_block.block.class">RedKiteCms\Block\TwitterBootstrapBundle\Core\Block\Navbar\AlBlockManagerBootstrapNavbarBlock</parameter>
        <parameter key="bootstrap_navbar_dropbown_block.block.class">RedKiteCms\Block\TwitterBootstrapBundle\Core\Block\Navbar\AlBlockManagerBootstrapNavbarDropdownBlock</parameter>
        <parameter key="bootstrap_navbar_dropbown.form.class">RedKiteCms\Block\TwitterBootstrapBundle\Core\Form\NavbarDropdown\AlNavbarDropdownType</parameter>
        
        <!-- BUTTON -->
        <parameter key="bootstrapbuttonblock.external_javascripts.cms" type="collection">
            <parameter>@TwitterBootstrapBundle/Resources/public/button/js/button.js</parameter>
        </parameter>
        
        <parameter key="bootstrap_button_block.block.class">RedKiteCms\Block\TwitterBootstrapBundle\Core\Block\Button\AlBlockManagerBootstrapButtonBlock</parameter>
        <parameter key="bootstrap_button_block.form.class">RedKiteCms\Block\TwitterBootstrapBundle\Core\Form\Button\AlButtonType</parameter>
        
        <!-- DROPDOWN BUTTON -->
        <parameter key="bootstrap_dropdown_button_block.block.class">RedKiteCms\Block\TwitterBootstrapBundle\Core\Block\DropdownButton\AlBlockManagerBootstrapDropdownButtonBlock</parameter>
        
        <parameter key="bootstrapdropdownbuttonblock.external_stylesheets.cms" type="collection">
            <parameter>@JstreeBundle/Resources/public/css/alphalemon_jstree.css</parameter>
        </parameter>
        
        <parameter key="bootstrapdropdownbuttonblock.external_javascripts.cms" type="collection">
            <parameter>@TwitterBootstrapBundle/Resources/public/dropdown_menu/js/dropdown_menu_editor.js</parameter>
        </parameter>
        
        <!-- SPLITDROPDOWN BUTTON -->
        <parameter key="bootstrap_split_dropdown_button_block.block.class">RedKiteCms\Block\TwitterBootstrapBundle\Core\Block\SplitDropdownButton\AlBlockManagerBootstrapSplitDropdownButtonBlock</parameter>
        
        <!-- BUTTONS -->
        <parameter key="bootstrapbuttonsgroupblock.external_javascripts.cms" type="collection">
            <parameter>@TwitterBootstrapBundle/Resources/public/buttons/js/buttons_group.js</parameter>
        </parameter>
        
        <parameter key="bootstrap_buttons_group_block.block.class">RedKiteCms\Block\TwitterBootstrapBundle\Core\Block\ButtonsGroup\AlBlockManagerBootstrapButtonsGroupBlock</parameter>
        
        <!-- THUMBNAIL -->        
        <parameter key="bootstrapthumbnailsblock.external_javascripts.cms" type="collection">
            <parameter>@TwitterBootstrapBundle/Resources/public/js/thumbnail_editor.js</parameter>
        </parameter>
        
        <parameter key="bootstrap_thumbnail.form.class">RedKiteCms\Block\TwitterBootstrapBundle\Core\Form\Thumbnail\AlThumbnailType</parameter>
        <parameter key="bootstrap_thumbnail.block.class">RedKiteCms\Block\TwitterBootstrapBundle\Core\Block\Thumbnail\AlBlockManagerBootstrapThumbnailBlock</parameter>        
        <parameter key="bootstrap_thumbnails.block.class">RedKiteCms\Block\TwitterBootstrapBundle\Core\Block\Thumbnails\AlBlockManagerBootstrapThumbnailsBlock</parameter>
        
        
    </parameters>

    <services>
        <!-- SLIDER -->
        <service id="bootstrap_slider_block.block" class="%bootstrap_slider_block.block.class%">
            <tag name="red_kite_cms.blocks_factory.block" description="Carousel" type="BootstrapSliderBlock" group="bootstrap,Twitter Bootstrap" />
            <argument type="service" id="service_container" />
        </service>
        
        <service id="bootstrapsliderblock.form" class="%bootstrapsliderblock.form.class%">
        </service>
        
        <!-- LABEL -->        
        <service id="bootstrap_label_block.block" class="%bootstrap_label_block.block.class%">
            <tag name="red_kite_cms.blocks_factory.block" description="Label" type="BootstrapLabelBlock" group="bootstrap,Twitter Bootstrap" />
            <argument type="service" id="service_container" />
        </service>
                
        <service id="bootstrap_label_block.form" class="%bootstrap_label_block.form.class%">
        </service>
        
        <!-- BADGE --> 
        <service id="bootstrap_badge_block.block" class="%bootstrap_badge_block.block.class%">
            <tag name="red_kite_cms.blocks_factory.block" description="Badge" type="BootstrapBadgeBlock" group="bootstrap,Twitter Bootstrap" />
            <argument type="service" id="service_container" />
        </service>
        
        <service id="bootstrap_badge_block.form" class="%bootstrap_badge_block.form.class%">
        </service>
        
        <!-- ACORDION -->
        <service id="bootstrap_accordion_block.block" class="%bootstrap_accordion_block.block.class%">
            <tag name="red_kite_cms.blocks_factory.block" description="Accordion" type="BootstrapAccordionBlock" group="bootstrap,Twitter Bootstrap" />
            <argument type="service" id="service_container" />
        </service>
        
        <!-- NAVBAR -->
        <service id="bootstrap_navbar_block.block" class="%bootstrap_navbar_block.block.class%">
            <tag name="red_kite_cms.blocks_factory.block" description="Navbar" type="BootstrapNavbarBlock" group="bootstrap,Twitter Bootstrap" />
            <argument type="service" id="service_container" />
        </service>
        
        <service id="bootstrap_navbar_dropbown_block" class="%bootstrap_navbar_dropbown_block.block.class%">
            <tag name="red_kite_cms.blocks_factory.block" description="Nav dropdown" type="BootstrapNavbarDropdownBlock" group="bootstrap,Twitter Bootstrap" />
            <argument type="service" id="service_container" />
        </service>
        
        <service id="bootstrap_navbar_dropbown.form" class="%bootstrap_navbar_dropbown.form.class%">
        </service>
        
        <!-- BUTTON -->
        <service id="bootstrap_button_block.form" class="%bootstrap_button_block.form.class%">
        </service>
        
        <service id="bootstrap_button_block.block" class="%bootstrap_button_block.block.class%">
            <tag name="red_kite_cms.blocks_factory.block" description="Button" type="BootstrapButtonBlock" group="bootstrap,Twitter Bootstrap" />
            <argument type="service" id="service_container" />
        </service>
        
        <!-- DROPDOWN BUTTON -->
        <service id="bootstrap_dropdown_button_block.block" class="%bootstrap_dropdown_button_block.block.class%">
            <tag name="red_kite_cms.blocks_factory.block" description="Dropdown button" type="BootstrapDropdownButtonBlock" group="bootstrap,Twitter Bootstrap" />
            <argument type="service" id="service_container" />
        </service>
        
        <!-- SPLITDROPDOWN BUTTON -->
        <service id="bootstrap_split_dropdown_button_block.block" class="%bootstrap_split_dropdown_button_block.block.class%">
            <tag name="red_kite_cms.blocks_factory.block" description="Split Dropdown Button" type="BootstrapSplitDropdownButtonBlock" group="bootstrap,Twitter Bootstrap" />
            <argument type="service" id="service_container" />
        </service>
        
        <!-- BUTTONS -->
        <service id="bootstrap_buttons_group_block.block" class="%bootstrap_buttons_group_block.block.class%">
            <tag name="red_kite_cms.blocks_factory.block" description="Buttons Group" type="BootstrapButtonsGroupBlock" group="bootstrap,Twitter Bootstrap" />
            <argument type="service" id="service_container" />
        </service>
        
        <!-- THUMBNAIL --> 
        <service id="bootstrap_thumbnail.block" class="%bootstrap_thumbnail.block.class%">
            <tag name="red_kite_cms.blocks_factory.block" description="Thumbnail" type="BootstrapThumbnailBlock" group="bootstrap,Twitter Bootstrap" />
            <argument type="service" id="service_container" />
        </service>
        
        <service id="bootstrap_thumbnails.block" class="%bootstrap_thumbnails.block.class%">
            <tag name="red_kite_cms.blocks_factory.block" description="Thumbnails list" type="BootstrapThumbnailsBlock" group="bootstrap,Twitter Bootstrap" />
            <argument type="service" id="service_container" />
        </service>
        
        <service id="bootstrap_thumbnail.form" class="%bootstrap_thumbnail.form.class%">
        </service>
    </services>
</container>
